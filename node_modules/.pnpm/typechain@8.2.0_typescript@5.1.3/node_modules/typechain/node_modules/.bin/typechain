#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/inkvisto/Desktop/programming/nextjs13-hardhat/node_modules/.pnpm/typechain@8.2.0_typescript@5.1.3/node_modules/typechain/dist/cli/node_modules:/home/inkvisto/Desktop/programming/nextjs13-hardhat/node_modules/.pnpm/typechain@8.2.0_typescript@5.1.3/node_modules/typechain/dist/node_modules:/home/inkvisto/Desktop/programming/nextjs13-hardhat/node_modules/.pnpm/typechain@8.2.0_typescript@5.1.3/node_modules/typechain/node_modules:/home/inkvisto/Desktop/programming/nextjs13-hardhat/node_modules/.pnpm/typechain@8.2.0_typescript@5.1.3/node_modules:/home/inkvisto/Desktop/programming/nextjs13-hardhat/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/inkvisto/Desktop/programming/nextjs13-hardhat/node_modules/.pnpm/typechain@8.2.0_typescript@5.1.3/node_modules/typechain/dist/cli/node_modules:/home/inkvisto/Desktop/programming/nextjs13-hardhat/node_modules/.pnpm/typechain@8.2.0_typescript@5.1.3/node_modules/typechain/dist/node_modules:/home/inkvisto/Desktop/programming/nextjs13-hardhat/node_modules/.pnpm/typechain@8.2.0_typescript@5.1.3/node_modules/typechain/node_modules:/home/inkvisto/Desktop/programming/nextjs13-hardhat/node_modules/.pnpm/typechain@8.2.0_typescript@5.1.3/node_modules:/home/inkvisto/Desktop/programming/nextjs13-hardhat/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli/cli.js" "$@"
else
  exec node  "$basedir/../../dist/cli/cli.js" "$@"
fi
