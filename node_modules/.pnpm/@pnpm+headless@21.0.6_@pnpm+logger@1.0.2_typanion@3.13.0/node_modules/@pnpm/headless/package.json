{
  "name": "@pnpm/headless",
  "description": "Fast installation using only pnpm-lock.yaml",
  "version": "21.0.6",
  "bugs": {
    "url": "https://github.com/pnpm/pnpm/issues"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "!*.map"
  ],
  "peerDependencies": {
    "@pnpm/logger": "^5.0.0"
  },
  "devDependencies": {
    "@pnpm/registry-mock": "3.8.0",
    "@types/fs-extra": "^9.0.13",
    "@types/ramda": "0.28.20",
    "@types/rimraf": "^3.0.2",
    "@types/sinon": "^10.0.15",
    "isexe": "2.0.0",
    "load-json-file": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "sinon": "^15.2.0",
    "tempy": "^1.0.1",
    "write-json-file": "^4.3.0",
    "@pnpm/assert-project": "2.3.28",
    "@pnpm/cafs": "7.0.4",
    "@pnpm/client": "10.0.12",
    "@pnpm/headless": "21.0.6",
    "@pnpm/package-store": "16.0.7",
    "@pnpm/prepare": "0.0.71",
    "@pnpm/read-projects-context": "8.0.5",
    "@pnpm/test-fixtures": "0.1.1",
    "@pnpm/store-path": "8.0.0"
  },
  "directories": {
    "test": "test"
  },
  "homepage": "https://github.com/pnpm/pnpm/blob/main/pkg-manager/headless#readme",
  "keywords": [
    "pnpm8",
    "pnpm",
    "install",
    "installer"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=16.14"
  },
  "repository": "https://github.com/pnpm/pnpm/blob/main/pkg-manager/headless",
  "dependencies": {
    "@zkochan/rimraf": "^2.1.2",
    "p-limit": "^3.1.0",
    "path-absolute": "^1.0.1",
    "path-exists": "^4.0.0",
    "ramda": "npm:@pnpm/ramda@0.28.1",
    "realpath-missing": "^1.1.0",
    "@pnpm/build-modules": "11.0.9",
    "@pnpm/constants": "7.1.1",
    "@pnpm/calc-dep-state": "4.1.0",
    "@pnpm/core-loggers": "9.0.1",
    "@pnpm/dependency-path": "2.1.2",
    "@pnpm/deps.graph-builder": "0.0.0",
    "@pnpm/filter-lockfile": "8.0.6",
    "@pnpm/hoist": "8.0.8",
    "@pnpm/lockfile-file": "8.1.1",
    "@pnpm/lockfile-to-pnp": "3.0.6",
    "@pnpm/link-bins": "9.0.4",
    "@pnpm/modules-cleaner": "14.0.8",
    "@pnpm/lockfile-utils": "8.0.2",
    "@pnpm/package-is-installable": "8.0.3",
    "@pnpm/lifecycle": "15.0.4",
    "@pnpm/modules-yaml": "12.1.1",
    "@pnpm/error": "5.0.2",
    "@pnpm/package-requester": "21.0.7",
    "@pnpm/pkg-manager.direct-dep-linker": "2.1.2",
    "@pnpm/read-package-json": "8.0.2",
    "@pnpm/read-project-manifest": "5.0.3",
    "@pnpm/symlink-dependency": "7.0.1",
    "@pnpm/real-hoist": "2.0.8",
    "@pnpm/store-controller-types": "15.0.1",
    "@pnpm/types": "9.1.0"
  },
  "funding": "https://opencollective.com/pnpm",
  "exports": {
    ".": "./lib/index.js"
  },
  "scripts": {
    "start": "tsc --watch",
    "commit": "commit",
    "commitmsg": "commitlint -e",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "registry-mock": "registry-mock",
    "test:jest": "jest",
    "test:e2e": "registry-mock prepare && run-p -r registry-mock test:jest",
    "_test": "cross-env PNPM_REGISTRY_MOCK_PORT=7773 pnpm run test:e2e",
    "test": "pnpm run compile && pnpm run _test",
    "runPrepareFixtures": "node ../../pnpm/bin/pnpm.cjs i -r -C test/fixtures --no-shared-workspace-lockfile --no-link-workspace-packages --lockfile-only --registry http://localhost:4873/ --ignore-scripts --force --no-strict-peer-dependencies",
    "prepareFixtures": "registry-mock prepare && run-p -r registry-mock runPrepareFixtures",
    "compile": "tsc --build && pnpm run lint --fix"
  }
}